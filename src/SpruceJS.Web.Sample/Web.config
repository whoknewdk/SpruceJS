<?xml version="1.0"?>

<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
	<configSections>
		<section name="SpruceJS" type="SpruceJS.Web.SpruceJSConfigurationSection,SpruceJS.Web" />
	</configSections>

	<SpruceJS minify="true" cache="true">
	</SpruceJS>

    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>
	<system.webServer>
		<handlers>
			<add name="SpruceJS" verb="*" path="*.spruce.js.map" type="SpruceJS.Web.SpruceJSHttpHandler,SpruceJS.Web" />
			<add name="SpruceJSMap" verb="*" path="*.spruce.js" type="SpruceJS.Web.SpruceJSHttpHandler,SpruceJS.Web" />
			<add name="SpruceJSUncached" verb="*" path="*.js" type="SpruceJS.Web.UncachedJSHttpHandler,SpruceJS.Web" />
		</handlers>
	</system.webServer>
</configuration>
